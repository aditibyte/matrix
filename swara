 #include<stdio.h>
 main()
{
 int i,j;
	int arr[3][3];
	int sum=0;
	printf("\n\nINPUT ELEMENTS:");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
		printf("\nelement - [%d],[%d] :" ,i,j);
		scanf("%d",&arr[i][j]);
	}
}
printf("\nThe matrix is :  \n");
for(i=0;i<3;i++)
{
	printf("\n");
	for(j=0;j<3;j++)
	{
	printf("%d\t",arr[i][j]);
    }
}
printf("\n\n");
for(i=0;i<3;i++)
{
	sum=sum+arr[i][i];
}
 printf("\nThe sum of diagonal elements is : %d",sum);
 return 0;
}
